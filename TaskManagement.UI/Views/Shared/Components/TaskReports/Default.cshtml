@model List<TaskReportListVM>

    <h5> Kayıtlı işlem adımları</h5>
<table class="table table-hover">
    <thead>

        <tr>
            <th>Tanım</th>
            <th>Açıklama</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model)
        {
            <tr>
                <td>@row.Definition</td>
                <td>@row.Detail</td>
                
            </tr>
        }





</table>
